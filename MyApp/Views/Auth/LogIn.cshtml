@model AuthModel

<div>
    <form asp-action="LogIn" method="post" class="text-center m-3">
        <h3>
            Log In
        </h3>
        <div>
            Login<input asp-for="Login" />
        </div>
        <div>
            Password<input asp-for="Password" />
        </div>
        <input class="btn btn-success" type="submit" value="Apply" />
    </form>
</div>
